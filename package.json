	"dependencies": {
		"@graphql-tools/schema": "^8.3.10",
		"@prisma/client": "^3.14.0",
		"apollo-server-errors": "^3.3.1",
		"apollo-server-lambda": "^3.6.7",
		"dayjs": "^1.11.2",
		"dotenv": "^16.0.0",
		"email-validator": "^2.0.4",
		"express": "^4.18.1",
